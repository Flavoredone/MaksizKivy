MDScreen:
    name: "cat4"
    md_bg_color: '#FFFFFF'

    MDGridLayout:
        cols:1
        spacing:dp(10)

        Image:
            source: 'assets/cataloge/v4.jpg'
            pos_hint: {"center_x": .5, "center_y": .5}
            allow_stretch: True
            keep_ratio: True

        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            MDGridLayout:
                cols: 1
                size_hint: 1, 5.33
                spacing: '0dp'
                Image:
                    source: "assets/cataloge/4_1data.jpg"
                    keep_ratio: True
                    pos_hint: {"center_x": .5, "center_y": .5}
                    #5.33
                    size_hint: 1, 3.3
                Image:
                    source: "assets/cataloge/4_2data.jpg"
                    keep_ratio: True
                    pos_hint: {"center_x": .5, "center_y": .5}
                    #5.33
                    size_hint: 1, 2.217

    MDGridLayout:
        cols: 2
        size_hint_y: .13
        pos_hint: {'top': 1}
        spacing: dp(160)
        MDCard:
            opacity: .01
            size_hint_y: .2
            on_press:
                root.manager.current = 'cat_menu'

        MDCard:
            opacity: .01
            on_press:
                root.manager.current = 'MainScreen'

    MDGridLayout:
        cols: 3
        size_hint_y: .3
        pos_hint: {'top': .86}
        spacing: dp(50)
        MDCard:
            opacity: .01
            size_hint_y: .2
            on_release:
                root.manager.current = 'cat3'


        MDCard:
            opacity: .01

        MDCard:
            opacity: .01
            on_release:
                root.manager.current = 'cat5'